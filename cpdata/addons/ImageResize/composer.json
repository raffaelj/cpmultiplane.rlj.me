{
    "name": "raffaelj/cockpit-imageresize",
    "type": "cockpit-module",
    "description": "Resize and optimize uploaded images automatically in Cockpit CMS",
    "keywords": ["cockpit", "image", "resize", "optimize"],
    "homepage": "https://github.com/raffaelj/cockpit_ImageResize",
    "license": "MIT",
    "authors": [
        {
            "name": "Raffael Jesche",
            "homepage": "https://www.rlj.me"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "spatie/image-optimizer": "^1.2"
    },
    "config": {
        "vendor-dir": "lib/vendor"
    },
    "extra": {
        "installer-name": "ImageResize"
    }
}
